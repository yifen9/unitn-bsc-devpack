{
  auto_https off
  admin off
}

:8080 {
  root * /home/coder/nav
  file_server

  handle_path /ide* {
    reverse_proxy 127.0.0.1:9000
  }

  handle_path /files* {
    reverse_proxy http://drive.unitn.clarelab.moe {
      header_down -X-Frame-Options
      header_down -Content-Security-Policy
    }
  }
}